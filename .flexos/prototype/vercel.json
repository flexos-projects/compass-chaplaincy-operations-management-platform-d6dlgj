{
  "rewrites": [
    { "source": "/", "destination": "/pages/dashboard-v1.html" },
    { "source": "/login", "destination": "/pages/login-v1.html" },
    { "source": "/users", "destination": "/pages/users-v1.html" },
    { "source": "/users/:id", "destination": "/pages/user-detail-v1.html" },
    { "source": "/duty-days", "destination": "/pages/duty-days-v1.html" },
    { "source": "/coverage", "destination": "/pages/coverage-v1.html" },
    { "source": "/stipends", "destination": "/pages/stipends-v1.html" },
    { "source": "/stipends/:id", "destination": "/pages/stipend-detail-v1.html" },
    { "source": "/reports", "destination": "/pages/reports-v1.html" },
    { "source": "/settings", "destination": "/pages/settings-v1.html" }
  ],
  "headers": [
    {
      "source": "/shared/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    }
  ]
}
